{
  "headers": [
    {
      "source": "/index.tsx",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/:path((?!api/|.*\\.).*)",
      "destination": "/index.html"
    }
  ]
}